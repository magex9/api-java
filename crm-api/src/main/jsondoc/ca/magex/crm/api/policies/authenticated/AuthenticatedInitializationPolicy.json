{
  "name": "AuthenticatedInitializationPolicy",
  "package": "ca.magex.crm.api.policies.authenticated",
  "imports": {
    "CRM_ADMIN": "ca.magex.crm.api.services.CrmAuthenticationService",
    "CrmConfigurationPolicy": "ca.magex.crm.api.policies",
    "BasicConfigurationPolicy": "ca.magex.crm.api.policies.basic",
    "CrmAuthenticationService": "ca.magex.crm.api.services",
    "CrmConfigurationService": "ca.magex.crm.api.services"
  },
  "type": "class",
  "modifiers": ["public"],
  "implements": ["CrmConfigurationPolicy"],
  "constructors": [{
    "name": "AuthenticatedInitializationPolicy",
    "modifiers": ["public"],
    "parameters": [
      {
        "name": "auth",
        "type": "CrmAuthenticationService"
      },
      {
        "name": "init",
        "type": "CrmConfigurationService"
      }
    ],
    "body": "{\n    this.auth = auth;\n    this.delegate = new BasicConfigurationPolicy(init);\n}"
  }],
  "fields": [
    {
      "name": "auth",
      "modifiers": ["private"],
      "type": "CrmAuthenticationService"
    },
    {
      "name": "delegate",
      "modifiers": ["private"],
      "type": "CrmConfigurationPolicy"
    }
  ],
  "methods": [
    {
      "name": "canInitialize",
      "modifiers": ["public"],
      "type": "boolean",
      "annotations": [{"name": "Override"}],
      "body": "{\n    return delegate.canInitialize();\n}"
    },
    {
      "name": "canReset",
      "modifiers": ["public"],
      "type": "boolean",
      "annotations": [{"name": "Override"}],
      "body": "{\n    if (!delegate.canReset()) {\n        return false;\n    }\n    return auth.isUserInRole(CRM_ADMIN);\n}"
    }
  ]
}