{
  "name": "CrmLookupRepository",
  "package": "ca.magex.crm.api.repositories",
  "imports": {
    "LookupsFilter": "ca.magex.crm.api.filters",
    "Paging": "ca.magex.crm.api.filters",
    "FilteredPage": "ca.magex.crm.api.system",
    "Identifier": "ca.magex.crm.api.system",
    "Lookup": "ca.magex.crm.api.system"
  },
  "type": "interface",
  "modifiers": ["public"],
  "methods": [
    {
      "name": "findLookups",
      "modifiers": ["public"],
      "type": {
        "class": "FilteredPage",
        "generics": ["Lookup"]
      },
      "parameters": [
        {
          "name": "filter",
          "type": "LookupsFilter"
        },
        {
          "name": "paging",
          "type": "Paging"
        }
      ]
    },
    {
      "name": "countLookups",
      "modifiers": ["public"],
      "type": "long",
      "parameters": [{
        "name": "filter",
        "type": "LookupsFilter"
      }]
    },
    {
      "name": "findLookup",
      "modifiers": ["public"],
      "type": "Lookup",
      "parameters": [{
        "name": "lookupId",
        "type": "Identifier"
      }]
    },
    {
      "name": "saveLookup",
      "modifiers": ["public"],
      "type": "Lookup",
      "parameters": [{
        "name": "lookup",
        "type": "Lookup"
      }]
    }
  ]
}