{
  "name": "AuthenticatedLookupPolicy",
  "package": "ca.magex.crm.api.policies.authenticated",
  "imports": {
    "SYS_ADMIN": "ca.magex.crm.api.services.CrmAuthenticationService",
    "CrmLookupPolicy": "ca.magex.crm.api.policies",
    "BasicLookupPolicy": "ca.magex.crm.api.policies.basic",
    "CrmAuthenticationService": "ca.magex.crm.api.services",
    "CrmLookupService": "ca.magex.crm.api.services",
    "Identifier": "ca.magex.crm.api.system"
  },
  "type": "class",
  "modifiers": ["public"],
  "implements": ["CrmLookupPolicy"],
  "constructors": [{
    "name": "AuthenticatedLookupPolicy",
    "modifiers": ["public"],
    "parameters": [
      {
        "name": "auth",
        "type": "CrmAuthenticationService"
      },
      {
        "name": "lookups",
        "type": "CrmLookupService"
      }
    ],
    "body": "{\n    this.auth = auth;\n    this.delegate = new BasicLookupPolicy(lookups);\n}"
  }],
  "fields": [
    {
      "name": "auth",
      "modifiers": ["private"],
      "type": "CrmAuthenticationService"
    },
    {
      "name": "delegate",
      "modifiers": ["private"],
      "type": "CrmLookupPolicy"
    }
  ],
  "methods": [
    {
      "name": "canCreateLookup",
      "modifiers": ["public"],
      "type": "boolean",
      "annotations": [{"name": "Override"}],
      "body": "{\n    if (!delegate.canCreateLookup()) {\n        return false;\n    }\n    /* only a CRM Admin can create a Lookup */\n    return auth.isUserInRole(SYS_ADMIN);\n}"
    },
    {
      "name": "canViewLookup",
      "modifiers": ["public"],
      "type": "boolean",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "lookup",
        "type": "String"
      }],
      "body": "{\n    if (!delegate.canViewLookup(lookup)) {\n        return false;\n    }\n    /* anybody can view a lookup */\n    return true;\n}"
    },
    {
      "name": "canViewLookup",
      "modifiers": ["public"],
      "type": "boolean",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "lookupId",
        "type": "Identifier"
      }],
      "body": "{\n    if (!delegate.canViewLookup(lookupId)) {\n        return false;\n    }\n    /* anybody can view a lookup */\n    return true;\n}"
    },
    {
      "name": "canUpdateLookup",
      "modifiers": ["public"],
      "type": "boolean",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "lookupId",
        "type": "Identifier"
      }],
      "body": "{\n    if (!delegate.canUpdateLookup(lookupId)) {\n        return false;\n    }\n    /* only a CRM Admin can update a Lookup */\n    return auth.isUserInRole(SYS_ADMIN);\n}"
    },
    {
      "name": "canEnableLookup",
      "modifiers": ["public"],
      "type": "boolean",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "lookupId",
        "type": "Identifier"
      }],
      "body": "{\n    if (!delegate.canEnableLookup(lookupId)) {\n        return false;\n    }\n    /* only a CRM Admin can enable a Lookup */\n    return auth.isUserInRole(SYS_ADMIN);\n}"
    },
    {
      "name": "canDisableLookup",
      "modifiers": ["public"],
      "type": "boolean",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "lookupId",
        "type": "Identifier"
      }],
      "body": "{\n    if (!delegate.canDisableLookup(lookupId)) {\n        return false;\n    }\n    /* only a CRM Admin can disable a Lookup */\n    return auth.isUserInRole(SYS_ADMIN);\n}"
    }
  ]
}