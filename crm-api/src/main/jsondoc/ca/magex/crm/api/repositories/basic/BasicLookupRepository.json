{
  "name": "BasicLookupRepository",
  "package": "ca.magex.crm.api.repositories.basic",
  "imports": {
    "Collectors": "java.util.stream",
    "Stream": "java.util.stream",
    "SerializationUtils": "org.apache.commons.lang3",
    "LookupsFilter": "ca.magex.crm.api.filters",
    "PageBuilder": "ca.magex.crm.api.filters",
    "Paging": "ca.magex.crm.api.filters",
    "CrmUpdateNotifier": "ca.magex.crm.api.observer",
    "CrmLookupRepository": "ca.magex.crm.api.repositories",
    "CrmStore": "ca.magex.crm.api.repositories",
    "FilteredPage": "ca.magex.crm.api.system",
    "Identifier": "ca.magex.crm.api.system",
    "Lookup": "ca.magex.crm.api.system"
  },
  "type": "class",
  "modifiers": ["public"],
  "implements": ["CrmLookupRepository"],
  "constructors": [{
    "name": "BasicLookupRepository",
    "modifiers": ["public"],
    "parameters": [
      {
        "name": "store",
        "type": "CrmStore"
      },
      {
        "name": "notifier",
        "type": "CrmUpdateNotifier"
      }
    ],
    "body": "{\n    this.store = store;\n    this.notifier = notifier;\n}"
  }],
  "fields": [
    {
      "name": "store",
      "modifiers": ["private"],
      "type": "CrmStore"
    },
    {
      "name": "notifier",
      "modifiers": ["private"],
      "type": "CrmUpdateNotifier"
    }
  ],
  "methods": [
    {
      "name": "apply",
      "modifiers": ["private"],
      "type": {
        "class": "Stream",
        "generics": ["Lookup"]
      },
      "parameters": [{
        "name": "filter",
        "type": "LookupsFilter"
      }],
      "body": "{\n    return store.getLookups().values().stream().filter(p -> filter.apply(p));\n}"
    },
    {
      "name": "findLookups",
      "modifiers": ["public"],
      "type": {
        "class": "FilteredPage",
        "generics": ["Lookup"]
      },
      "annotations": [{"name": "Override"}],
      "parameters": [
        {
          "name": "filter",
          "type": "LookupsFilter"
        },
        {
          "name": "paging",
          "type": "Paging"
        }
      ],
      "body": "{\n    return PageBuilder.buildPageFor(filter, apply(filter).map(i -> SerializationUtils.clone(i)).sorted(filter.getComparator(paging)).collect(Collectors.toList()), paging);\n}"
    },
    {
      "name": "countLookups",
      "modifiers": ["public"],
      "type": "long",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "filter",
        "type": "LookupsFilter"
      }],
      "body": "{\n    return apply(filter).count();\n}"
    },
    {
      "name": "findLookup",
      "modifiers": [
        "public",
        "synchronized"
      ],
      "type": "Lookup",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "lookupId",
        "type": "Identifier"
      }],
      "body": "{\n    return store.getLookups().get(lookupId);\n}"
    },
    {
      "name": "saveLookup",
      "modifiers": ["public"],
      "type": "Lookup",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "lookup",
        "type": "Lookup"
      }],
      "body": "{\n    notifier.lookupUpdated(System.nanoTime(), lookup.getLookupId());\n    store.getLookups().put(lookup.getLookupId(), lookup);\n    return lookup;\n}"
    }
  ]
}