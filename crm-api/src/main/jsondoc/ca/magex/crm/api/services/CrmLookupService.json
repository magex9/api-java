  "methods": [
    {
      "name": "prototypeLookup",
      "modifiers": ["default"],
      "type": "Lookup",
      "parameters": [
        {
          "name": "name",
          "type": "Localized"
        },
        {
          "name": "parent",
          "type": "Option"
        }
      ],
      "body": "{\n    return new Lookup(null, Status.PENDING, true, name, parent);\n}"
    },
    {
      "name": "createLookup",
      "modifiers": ["default"],
      "type": "Lookup",
      "parameters": [{
        "name": "lookup",
        "type": "Lookup"
      }],
      "body": "{\n    return createLookup(lookup.getName(), lookup.getParent());\n}"
    },
    {
      "name": "createLookup",
      "type": "Lookup",
      "parameters": [
        {
          "name": "name",
          "type": "Localized"
        },
        {
          "name": "parent",
          "type": "Option"
        }
      ]
    },
    {
      "name": "findLookup",
      "type": "Lookup",
      "parameters": [{
        "name": "lookupId",
        "type": "Identifier"
      }]
    },
    {
      "name": "updateLookupName",
      "type": "Lookup",
      "parameters": [
        {
          "name": "lookupId",
          "type": "Identifier"
        },
        {
          "name": "name",
          "type": "Localized"
        }
      ]
    },
    {
      "name": "findLookupByCode",
      "modifiers": ["default"],
      "type": "Lookup",
      "parameters": [{
        "name": "code",
        "type": "String"
      }],
      "body": "{\n    return (Lookup) findLookups(defaultLookupsFilter().withLookupCode(code), LookupsFilter.getDefaultPaging()).getSingleItem();\n}"
    },
    {
      "name": "findLookupByTypeWithParent",
      "modifiers": ["default"],
      "type": "Lookup",
      "parameters": [{
        "name": "parent",
        "type": "Lookup"
      }],
      "body": "{\n    return (Lookup) findLookups(defaultLookupsFilter().withParentCode(parent.getCode()), LookupsFilter.getDefaultPaging()).getSingleItem();\n}"
    },
    {
      "name": "enableLookup",
      "type": "Lookup",
      "parameters": [{
        "name": "lookupId",
        "type": "Identifier"
      }]
    },
    {
      "name": "disableLookup",
      "type": "Lookup",
      "parameters": [{
        "name": "lookupId",
        "type": "Identifier"
      }]
    },
    {
      "name": "defaultLookupsFilter",
      "modifiers": ["default"],
      "type": "LookupsFilter",
      "body": "{\n    return new LookupsFilter();\n}"
    },
    {
      "name": "findLookups",
      "type": {
        "class": "FilteredPage",
        "generics": ["Lookup"]
      },
      "parameters": [
        {
          "name": "filter",
          "type": "LookupsFilter"
        },
        {
          "name": "paging",
          "type": "Paging"
        }
      ]
    },
    {
      "name": "findLookups",
      "modifiers": ["default"],
      "type": {
        "class": "FilteredPage",
        "generics": ["Lookup"]
      },
      "parameters": [{
        "name": "filter",
        "type": "LookupsFilter"
      }],
      "body": "{\n    return findLookups(filter, defaultLookupPaging());\n}"
    },
    {
      "name": "defaultLookupPaging",
      "modifiers": ["default"],
      "type": "Paging",
      "body": "{\n    return new Paging(LookupsFilter.getSortOptions().get(0));\n}"
    }
  ]
}