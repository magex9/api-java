{
  "name": "BasicOptionService",
  "package": "ca.magex.crm.api.services.basic",
  "imports": {
    "OptionsFilter": "ca.magex.crm.api.filters",
    "Paging": "ca.magex.crm.api.filters",
    "CrmRepositories": "ca.magex.crm.api.repositories",
    "CrmOptionService": "ca.magex.crm.api.services",
    "FilteredPage": "ca.magex.crm.api.system",
    "Identifier": "ca.magex.crm.api.system",
    "Localized": "ca.magex.crm.api.system",
    "Option": "ca.magex.crm.api.system",
    "Status": "ca.magex.crm.api.system"
  },
  "type": "class",
  "modifiers": ["public"],
  "implements": ["CrmOptionService"],
  "constructors": [{
    "name": "BasicOptionService",
    "modifiers": ["public"],
    "parameters": [{
      "name": "repos",
      "type": "CrmRepositories"
    }],
    "body": "{\n    this.repos = repos;\n}"
  }],
  "fields": [{
    "name": "repos",
    "modifiers": ["private"],
    "type": "CrmRepositories"
  }],
  "methods": [
    {
      "name": "createOption",
      "modifiers": ["public"],
      "type": "Option",
      "annotations": [{"name": "Override"}],
      "parameters": [
        {
          "name": "lookupId",
          "type": "Identifier"
        },
        {
          "name": "name",
          "type": "Localized"
        }
      ],
      "body": "{\n    return repos.saveOption(new Option(repos.generateId(), lookupId, Status.ACTIVE, name));\n}"
    },
    {
      "name": "findOption",
      "modifiers": ["public"],
      "type": "Option",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "optionId",
        "type": "Identifier"
      }],
      "body": "{\n    return repos.findOption(optionId);\n}"
    },
    {
      "name": "updateOptionName",
      "modifiers": ["public"],
      "type": "Option",
      "annotations": [{"name": "Override"}],
      "parameters": [
        {
          "name": "optionId",
          "type": "Identifier"
        },
        {
          "name": "name",
          "type": "Localized"
        }
      ],
      "body": "{\n    Option option = repos.findOption(optionId);\n    if (option == null) {\n        return null;\n    }\n    return repos.saveOption(option.withName(name));\n}"
    },
    {
      "name": "enableOption",
      "modifiers": ["public"],
      "type": "Option",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "optionId",
        "type": "Identifier"
      }],
      "body": "{\n    Option option = repos.findOption(optionId);\n    if (option == null) {\n        return null;\n    }\n    return repos.saveOption(option.withStatus(Status.ACTIVE));\n}"
    },
    {
      "name": "disableOption",
      "modifiers": ["public"],
      "type": "Option",
      "annotations": [{"name": "Override"}],
      "parameters": [{
        "name": "optionId",
        "type": "Identifier"
      }],
      "body": "{\n    Option option = repos.findOption(optionId);\n    if (option == null) {\n        return null;\n    }\n    return repos.saveOption(option.withStatus(Status.INACTIVE));\n}"
    },
    {
      "name": "findOptions",
      "modifiers": ["public"],
      "type": {
        "class": "FilteredPage",
        "generics": ["Option"]
      },
      "annotations": [{"name": "Override"}],
      "parameters": [
        {
          "name": "filter",
          "type": "OptionsFilter"
        },
        {
          "name": "paging",
          "type": "Paging"
        }
      ],
      "body": "{\n    return repos.findOptions(filter, paging);\n}"
    }
  ]
}