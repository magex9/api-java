          "type": "String"
        },
        {
          "name": "name",
          "type": "PersonName"
        },
        {
          "name": "email",
          "type": "String"
        },
        {
          "name": "username",
          "type": "String"
        },
        {
          "name": "password",
          "type": "String"
        }
      ]
    },
    {
