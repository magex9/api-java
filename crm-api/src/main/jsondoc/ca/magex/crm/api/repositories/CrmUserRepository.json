{
  "name": "CrmUserRepository",
  "package": "ca.magex.crm.api.repositories",
  "imports": {
    "Paging": "ca.magex.crm.api.filters",
    "UsersFilter": "ca.magex.crm.api.filters",
    "User": "ca.magex.crm.api.roles",
    "FilteredPage": "ca.magex.crm.api.system",
    "Identifier": "ca.magex.crm.api.system"
  },
  "type": "interface",
  "modifiers": ["public"],
  "methods": [
    {
      "name": "findUsers",
      "modifiers": ["public"],
      "type": {
        "class": "FilteredPage",
        "generics": ["User"]
      },
      "parameters": [
        {
          "name": "filter",
          "type": "UsersFilter"
        },
        {
          "name": "paging",
          "type": "Paging"
        }
      ]
    },
    {
      "name": "countUsers",
      "modifiers": ["public"],
      "type": "long",
      "parameters": [{
        "name": "filter",
        "type": "UsersFilter"
      }]
    },
    {
      "name": "findUser",
      "modifiers": ["public"],
      "type": "User",
      "parameters": [{
        "name": "userId",
        "type": "Identifier"
      }]
    },
    {
      "name": "saveUser",
      "modifiers": ["public"],
      "type": "User",
      "parameters": [{
        "name": "user",
        "type": "User"
      }]
    }
  ]
}